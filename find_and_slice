# load and read text
print ("Load and read text...")
OpenFile = open("constitution_of_usa.txt")
ReadText = OpenFile.read()

# remove new lines and tabs
print ("Remove new lines and tabs ...")
RawText = ReadText.replace("\n", " ").replace("\t", " ")


findWord = RawText.find("We")
print (findWord)

Text = RawText[58:158]
print (Text)
